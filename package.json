{
  "name": "wtskorea",
  "version": "1.0.0",
  "description": "Prototype Website WTSKorea",
  "main": "index.js",
  "author": "David",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.910.0",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.2.4",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^3.1.0",
    "helmet": "^4.4.1",
    "mini-css-extract-plugin": "^1.6.0",
    "morgan": "^1.10.0",
    "multer-s3": "^2.9.0",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.2.0",
    "pug": "^3.0.2",
    "sass-loader": "^11.0.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build && cp -R src/images build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "webpack-dev-server": "^3.11.2"
  },
  "homepage": "https://david0816.github.io/wtskorea/"
}
